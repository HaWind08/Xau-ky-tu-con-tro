#include<iostream>
#include<string.h>

using namespace std;

bool Check(char S[])
{
	int count = 0;
	for(int i = 0; i < strlen(S);i++)
	{
		if(S[i] == ' ' && S[i + 1] == ' ') return false;
		count++;
	}
	if(count > 10) return false;
	
	return true;
	
}
int main()
{
	char S[200]; int count;
	do
	{
		cout << "Nhap xau ky tu: ";
		cin.getline(S,50);
	} while(strlen(S) > 50);
	
	if(Check(S))
	{
		cout << "Xau hop le.";
	}
	else cout << "Xau khong hop le.";
	
	return 0;
	
}
