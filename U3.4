#include<iostream>
#include<string.h>

using namespace std;

void Nhap(char S[])
{
	cout << "Nhap xau ky tu: ";
	cin.getline(S,30);
}

bool check(char S[])
{
	int M = 0, D = 0;
	for( int i = 0; i < strlen(S);i++)
	{
		if(S[i] == '(' ) M++;
		if(S[i] == ')' ) D++;
		if(M < D) return false;
	}
	if(M != D) return false;
	
	return true;
}

int main()
{
	char S[200];
	
	Nhap(S);
	
	if(check(S))
	{
		cout << "Xau hop le.";
	}
	else cout << "Xau khong hop le.";
	
	return 0;
}


